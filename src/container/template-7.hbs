{{!-- 
Це код container файлу для template-7 сторінки, 
в якому міститься код, котрий буде віддаватися по шляху (PATH) - "/"
 --}}

    <header>
        {{#with header}}
        <div class="header">
        <div>
          {{>logo logo=brandName}}
        </div> 
           <nav class="header-nav  header_nav_right"> 
           {{>nav list=links}}
          </nav>
        </div>
         <div class="head">
          <div>
             <span>Ag</span>
             {{#with ad}}
              <a href="{{url}}"class="nav_link">{{text}}:{{url}}</a>
            {{/with}}
          </div>
            <p>Users Online: {{userOnline}}</p>
             
          </div>
     {{/with}} 
    </header>
     
    <main class="main">
      {{#with hero}}
      <div class="hero">
        <h1 class="hero_title">{{title}}</h1>
        <p class="hero_text">{{description}}</p>
        {{#with button}}
        <button class="button hero_button" href="{{url}}">{{text}}</button>
        {{/with}}
      </div>
      {{/with}}
      {{#with about}}
      <div class="about">
        <strong class="about_title">{{heading}}</strong>
        <p class="about_text">{{info}}</p>
      </div>
      {{/with}}

    </main>
    <section class="articless">
    <div class="articles_head">
      <h2 class="articles_title">Our Articles</h2>
      <button href="#" class="button articless_head_button"><p>View All</p></button>
    </div>
    <div class="blog">

    
    {{>articleCardDark data=articles}}
    </div>
    </section>
 

   
 
